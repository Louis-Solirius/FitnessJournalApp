<div class="nested-form-wrapper" data-new-record="<%= f.object.new_record? %>">
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :sets %>
  <%= f.number_field :sets %>

  <%= f.label :reps %>
  <%= f.number_field :reps %>

  <%= f.label :rpe %>
  <%= f.number_field :rpe %>

  <%= f.label :weight %>
  <%= f.number_field :weight %>

  <button type="button" data-action="nested-form#remove">Remove exercise</button>

  <%= f.hidden_field :_destroy %>
</div>
