<p id="notice"><%= notice %></p>

<div class="card">
  <div class="card-header">
    <h3>Edit Workout</h3>
  </div>
  <div class="card-body">
    <%= render partial: "form", locals: {workout: @workout} %>
    <%= button_to "Delete Workout", @workout, method: :delete, data: {turbo_confirm: "Are you sure you want to delete this workout?"}, class:"btn btn-outline-danger m-3"%>
  </div>
</div>


<%= link_to 'Back', :back, class:"btn btn-secondary m-3" %>

