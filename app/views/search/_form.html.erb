<div class="card">
    <div class="card-header">
        <h3>Workouts</h3>
    </div>

    <div class="card-body">
        <%= search_form_for(@query, url: search_path, method: :get, class:'d-flex') do |f| %>
            <%= f.search_field :title_i_cont_any, placeholder: "Search Workout", class:"form-control me-2" %>
            <%= f.submit "Search", class:"btn btn-outline-success" %>
        <% end %>
    </div>
</div>

